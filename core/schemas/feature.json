{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://beta.schemas.opengis.net/json-fg/feature.json",
  "title": "a JSON-FG Feature",
  "description": "This JSON Schema is part of JSON-FG version 0.3.0",
  "type": "object",
  "required": ["type", "geometry", "properties"],
  "properties": {
    "conformsTo": {
      "$ref": "conformsto.json"
    },
    "type": {
      "const": "Feature"
    },
    "id": {
      "oneOf": [
        {
          "type": "number"
        },
        {
          "type": "string"
        }
      ]
    },
    "featureType": {
      "$ref": "featuretype.json"
    },
    "featureSchema": {
      "$ref": "featureschema.json"
    },
    "time": {
      "$ref": "time.json"
    },
    "coordRefSys": {
      "$ref": "coordrefsys.json"
    },
    "measures": {
      "$ref": "measures.json"
    },
    "place": {
      "$ref": "place.json"
    },
    "geometry": {
      "$ref": "geometry.json"
    },
    "properties": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "object"
        }
      ]
    }
  }
}
